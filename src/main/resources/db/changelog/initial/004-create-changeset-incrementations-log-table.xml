<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

   <changeSet id="create_table_forests" author="n.erko">
       <createTable tableName="Incrementation_Log">
           <column name="log_id" type="SERIAL">
               <constraints primaryKey="true" nullable="false"/>
           </column>
           <column name="tree_id" type="INT">
               <constraints foreignKeyName="fk3" referencedTableName="Trees" referencedColumnNames="tree_id"/>
           </column>
           <column name="increment_date" type="TIMESTAMP"/>
           <column name="increment_value" type="VARCHAR(255)"/>
       </createTable>
   </changeSet>
</databaseChangeLog>